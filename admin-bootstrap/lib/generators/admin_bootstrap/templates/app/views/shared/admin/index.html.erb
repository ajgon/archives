<div class="page-header">
  <h1><%= humanize(@model) %></h1>
</div>
<div class="row-fluid">
    <table class="table table-bordered table-striped table-data" data-name="<%= @model.to_s %>" data-disabled-actions="<%= Array(@model.admin_option_value(:disabled_actions)).join(' ') %>">
        <thead>
            <tr>
<% @model.columns(:admin => true).each do |column| %>
                <%= render 'headers', :column => column %>
<% end %>
                <th></th>
            </tr>
        </thead>
        <tbody></tbody>
        <tfoot>
            <tr>
<% @model.columns(:admin => true).each do |column| %>
                <%= render 'headers', :column => column %>
<% end %>
                <th></th>
            </tr>
        </tfoot>
    </table>
</div>